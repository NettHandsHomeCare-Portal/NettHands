    "version": "0.2.0",
    "configurations": [



        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "python": "${command:python.interpreterPath}",
            "program": "${workspaceFolder}/nhhc/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,

            "justMyCode": true,
            "autoReload": {"enable": true},
            "host": "localhost",
            "port": 9000,
            "preLaunchTask": "start venv",
            "env": {
                "DOPPLER_ENV":"1"
            }
    
        }
    ]
}
